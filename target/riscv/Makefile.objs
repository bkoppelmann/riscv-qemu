obj-y += translate.o op_helper.o helper.o cpu.o fpu_helper.o \
	user_atomic.o gdbstub.o pmp.o

DECODETREE = $(SRC_PATH)/scripts/decodetree.py

target/riscv/decode-rvi.inc.c: $(SRC_PATH)/target/riscv/rv32I.def $(DECODETREE)
